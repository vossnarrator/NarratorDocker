{
  "name": "node-ws-test",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku-examples/node-ws-test.git"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "discord.js": "^11.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.3",
    "express-ws": "^4.0.0",
    "firebase": "^3.9.0",
    "firebase-admin": "^4.2.1",
    "glob": "^7.1.3",
    "js-md5": "^0.7.3",
    "jsonschema": "^1.2.4",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.2",
    "moment": "^2.24.0",
    "mysql": "^2.15.0",
    "node-gcm": "^0.14.10",
    "request": "^2.88.0",
    "sinon": "^7.2.5",
    "string-similarity": "^1.2.0",
    "underscore": "^1.9.1",
    "ws": "^1.1.5"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "buildJS": "node node_modules/browserify/bin/cmd.js static/js/index.js -o public/js/index.js && ./node_modules/.bin/babel --presets=@babel/env public/js/index.js --out-dir public/js",
    "dev": "nodemon --ignore 'public/*' --ignore 'static/*' web/runner.js",
    "devlint": "eslint --ext .js ./web --fix",
    "devServerNoInternet": "nodemon --ignore 'public/*' --ignore 'static/*' web/runner.js --no-internet",
    "lint": "eslint --ext .js ./web",
    "mocha": "mocha web/test/integration/** --exit --timeout 15000",
    "start": "node server.js",
    "watch": "watchify static/js/index.js > ./node_modules/.bin/babel --presets=@babel/env -o public/js/index.js"
  }
}
